{
  "name": "atom-antescofo",
  "main": "./lib/language-antescofo",
  "version": "2.2.1",
  "description": "Antescofo syntax grammar core to activate syntax highlighting",
  "keywords": [
    ".asco",
    ".asco.txt"
  ],
  "activationCommands": {
    "atom-workspace": [
      "language-antescofo:toggle",
      "antescofo:startantescofo",
      "antescofo:oscsend",
      "antescofo:startfromlabel",
      "antescofo:stoptantescofo",
      "antescofo:Loadantescofo",
      "antescofo:nextevent"
    ]
  },
  "repository": "https://github.com/nadirB/atom-antescofo",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "node-osc": "^2.1.0"
  }
}
